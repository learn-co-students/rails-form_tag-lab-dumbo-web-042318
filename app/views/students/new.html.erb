
<%= form_tag students_path do%>
  <h1>Student Form</h1>
  <%= label_tag("student[first_name]", 'Student First Name')%>
  <%= text_field_tag('student[first_name]')%><br>

  <%= label_tag("student[last_name]", 'Student Last Name')%>
  <%= text_field_tag('student[last_name]')%><br>



  <%= submit_tag 'Submit yo student'%>


<%end%>
